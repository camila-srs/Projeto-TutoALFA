<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MenuControle">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Cadastros">
            <items>
              <MenuItem id="meuItemCadastrarColaborador" mnemonicParsing="false" onAction="#abrirCadastrarColaborador" text="Cadastrar Colaborador" />
                  <MenuItem id="menuItemCadastrarCurso" mnemonicParsing="false" onAction="#abrirCadastrarCurso" text="Cadastrar Curso" />
                  <MenuItem id="menuItemCadastrarDepartamento" mnemonicParsing="false" onAction="#abrirCadastrarDepartamento" text="Cadastrar Departamento" />
                  <MenuItem id="menuItemCadastrarCategoria" mnemonicParsing="false" onAction="#abrirCadastrarCategoria" text="Cadastrar Categoria" />
                  <MenuItem id="menuItemCadastarUniversidade" mnemonicParsing="false" onAction="#abrirCadastrarUniversidade" text="Cadastrar Universidade" />
                  <MenuItem id="menuItemCadastrarOcorrencia" mnemonicParsing="false" onAction="#abrirCadastrarOcorrencia" text="Cadastrar Ocorrencia" />
                  <MenuItem id="menuItemCadastrarTutoria" mnemonicParsing="false" onAction="#abrirCadastarTutoria" text="Cadastrar Tutoria" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
   <Pane fx:id="paneInicial" prefHeight="600.0" prefWidth="400.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
